<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>Koan.Service.Inbox.Connector.Redis</AssemblyName>
    <RootNamespace>Koan.Service.Inbox.Connector.Redis</RootNamespace>
    <Description>Redis-backed inbox service for Koan: lightweight HTTP receiver storing messages in Redis.</Description>
    <PackageTags>$(CommonPackageTags);service;inbox;redis;messaging</PackageTags>
    <Version>0.6.3</Version>
    <AssemblyVersion>0.6.3</AssemblyVersion>
    <FileVersion>0.6.3</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="2.9.25" />
    <PackageReference Include="RabbitMQ.Client" Version="7.1.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
  </ItemGroup>
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
    <ProjectReference Include="..\..\..\..\Koan.Core\Koan.Core.csproj" />
    <ProjectReference Include="..\..\..\..\Connectors\Data\Redis\Koan.Data.Connector.Redis.csproj" />
  </ItemGroup>
</Project>
