<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Data abstractions for Koan: repository contracts, entities, and filtering interfaces.</Description>
    <PackageTags>$(CommonPackageTags);data;abstractions;repository;entities</PackageTags>
    <Version>0.6.3</Version>
    <AssemblyVersion>0.6.3</AssemblyVersion>
    <FileVersion>0.6.3</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
    <PackageReference Include="Microsoft.AspNetCore.JsonPatch" Version="8.0.8" />
  </ItemGroup>
</Project>