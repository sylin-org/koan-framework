{
	"Koan": {
		"Flow": {
			"Materialization": {
				"DefaultPolicy": "last",
				"PerModelDefaults": {
					"S8.Canon.Shared.Device": "coalesce",
					"S8.Canon.Shared.Sensor": "coalesce"
				},
				"PerPath": {
					"S8.Canon.Shared.Device:device.identifier.serial": "first",
					"S8.Canon.Shared.Sensor:sensor.unit": "first"
				}
			}
		},
		"Messaging": {
			"DefaultBus": "rabbit",
			"Buses": {
				"rabbit": {
					"Provider": "RabbitMq",
					"ConnectionString": "amqp://guest:guest@localhost:5672",
					"RabbitMq": {
						"Exchange": "Koan",
						"Prefetch": 100,
						"ProvisionOnStart": true,
						"Subscriptions": {
							"0": {
								"Name": "flow",
								"RoutingKeys": "#",
								"Dlq": true,
								"Concurrency": 1
							}
						}
					}
				}
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Koan.Data.Mongo.MongoRepository": "Information",
			"Koan.Messaging.RabbitMq.RabbitMqProvider": "Information"
		}
	}
}
