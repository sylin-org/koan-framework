{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Koan": {
    "AllowMagicInProduction": false,
    "Data": {
      "DefaultProvider": "Mongo",
      "Mongo": {
        "ConnectionString": "mongodb://localhost:27017",
        "Database": "s9_location"
      }
    },
    "Messaging": {
      "DefaultBus": "rabbit",
      "Buses": {
        "rabbit": {
          "Provider": "RabbitMq",
          "ConnectionStringName": "rabbitmq",
          "RabbitMq": {
            "Exchange": "s9-location",
            "ExchangeType": "topic",
            "PublisherConfirms": true
          },
          "Prefetch": 50,
          "Dlq": { "Enabled": true },
          "ProvisionOnStart": true
        }
      }
    },
    "Ai": {
      "AutoDiscoveryEnabled": true,
      "AllowDiscoveryInNonDev": true
    }
  },
  "S9": {
    "Location": {
      "AiAssist": {
        "Enabled": false,
        "Model": "mistral",
        "ConfidenceThreshold": 0.85
      }
    }
  },
  "ConnectionStrings": {
    "rabbitmq": "amqp://guest:guest@localhost:5672"
  }
}
